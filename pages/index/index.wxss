.shixiquan {
  padding-bottom: 200rpx;
}
.searchBox {
  display: flex;
  justify-content: left;
  align-items: center;
  padding: 10rpx 56rpx 34rpx 48rpx;
  background: linear-gradient(#2f88e5, #4eb7f3);
}
.searchBox .chooseCity {
  height: 78rpx;
  width: 120rpx;
  color: #fff;
  font-size: 28rpx;
  text-align: left;
  vertical-align: middle;
  line-height: 78rpx;
  overflow: hidden;
  margin-right: 20rpx;
}
.searchBox .chooseCity text {
  color: #fff;
  font-size: 28rpx;
}
.searchBox .search {
  background-color: #fff;
  border-radius: 78rpx;
  display: flex;
  align-items: center;
  width: 544rpx;
  height: 78rpx;
}
.searchBox .search input {
  font-size: 28rpx;
}
.searchBox .search image {
  width: 30rpx;
  height: 30rpx;
  margin-left: 28rpx;
  margin-right: 14rpx;
}
.searchBox .search .phcolor {
  color: rgba(192, 192, 192);
}
.searchBox .search .reset {
  text-align: center;
  color: #fff;
}
.searchBox .search .reset text {
  font-size: 50rpx;
}
.searchBox .reset {
  width: 50rpx;
  height: 50rpx;
  text-align: center;
  color: #fff;
  margin-left: 20rpx;
}
.searchBox .reset text {
  font-size: 50rpx;
}
.get_userinfo_btn {
  background: linear-gradient(#2f88e5, #4eb7f3);
  height: 118rpx;
}
.swiper {
  transform: translateY(0);
  overflow: hidden;
}
.swiper swiper {
  width: 680rpx;
  height: 236rpx;
  transform: translateY(0);
  margin: 44rpx auto 34rpx;
  overflow: hidden;
}
.swiper swiper swiper-item {
  overflow: hidden;
}
.swiper swiper swiper-item image {
  width: 100%;
}
tabs .loading .pimage {
  display: block;
  width: 50%;
  margin: 75rpx auto;
}
tabs .loading .ptext {
  color: #5c5a58;
  text-align: center;
}
tabs .job {
  border-bottom: 4rpx solid rgba(92, 90, 88, 0.1);
  padding: 23rpx 50rpx;
}
tabs .job .jobTitle {
  font-size: 34rpx;
  margin-bottom: 10rpx;
  font-weight: 500;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
tabs .job .jobDetail {
  margin-bottom: 20rpx;
  font-size: 28rpx;
  color: #5c5a58;
}
tabs .job .jobTags {
  display: flex;
  margin-bottom: 24rpx;
}
tabs .job .jobTags view {
  width: 116rpx;
  height: 44rpx;
  border-radius: 12rpx;
  font-size: 22rpx;
  color: #fff;
  line-height: 44rpx;
  text-align: center;
  background-color: #a9c5e3;
  margin-right: 28rpx;
}
tabs .job .silkbag {
  display: flex;
  align-items: center;
  height: 52rpx;
}
tabs .job .silkbag .portrait image {
  width: 52rpx;
  height: 52rpx;
  border-radius: 50%;
}
tabs .job .silkbag .bagContent {
  margin-left: 20rpx;
  color: #5c5a58;
  font-size: 24rpx;
  height: 34rpx;
  line-height: 34rpx;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
