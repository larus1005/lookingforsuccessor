.tips {
  padding: 0 50rpx;
  display: flex;
  align-items: center;
  background-color: #e6f7ff;
  color: #2588e5;
  font-size: 28rpx;
  height: 64rpx;
  border: 2rpx solid #92d5ff;
}
.tips .content {
  margin-left: 30rpx;
}
.tips .close {
  color: #5c5a58;
  margin-left: auto;
}
.myRelease .job {
  border-bottom: 4rpx solid rgba(92, 90, 88, 0.1);
  padding: 23rpx 50rpx;
}
.myRelease .job .jobTitle {
  font-size: 34rpx;
  margin-bottom: 10rpx;
  display: flex;
}
.myRelease .job .jobTitle .online {
  margin-left: auto;
}
.myRelease .job .jobDetail {
  margin-bottom: 20rpx;
  font-size: 24rpx;
  color: #5c5a58;
}
.myRelease .job .jobTags {
  display: flex;
}
.myRelease .job .jobTags view {
  width: 116rpx;
  height: 44rpx;
  border-radius: 12rpx;
  font-size: 22rpx;
  color: #fff;
  line-height: 44rpx;
  text-align: center;
  background-color: #a9c5e3;
  margin-right: 28rpx;
}
.myRelease .job .silkbag {
  display: flex;
  align-items: center;
  height: 52rpx;
}
.myRelease .job .silkbag .portrait image {
  width: 52rpx;
  height: 52rpx;
  border-radius: 50%;
}
.myRelease .job .silkbag .bagContent {
  margin-left: 20rpx;
  color: #5c5a58;
  font-size: 24rpx;
  height: 34rpx;
  line-height: 34rpx;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
